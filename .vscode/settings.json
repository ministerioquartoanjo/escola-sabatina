{
    "markdown.styles": [
        //"https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css",
        "markdown-pdf.css",
    ],
    "[markdown]": {
        "files.trimTrailingWhitespace": false, // use 2 espa√ßos no final para ter uma quebra de linha
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "markdownExtended.pdfHeaderTemplate": "<div style=\"font-size: 9px; margin: 0 auto;\"></div>",
    "markdownExtended.pdfFooterTemplate": "<div style=\"font-size: 8px; margin: 0 auto;\"> <span class='pageNumber'></span></div>",
    "markdownExtended.disabledPlugins": "toc",
    "markdownAutoTOC.includeLevel": [1]
    "markdownAutoTOC.containerHeaderHtml": ""
}